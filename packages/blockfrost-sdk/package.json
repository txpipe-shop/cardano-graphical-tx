{
  "name": "@laceanatomy/blockfrost-sdk",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "gen": "docker run --rm -v ${PWD}/src:/local openapitools/openapi-generator-cli generate -i /local/openapi.yml -g typescript-axios -o /local"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@laceanatomy/typescript-config": "workspace:*",
    "typescript": "^5.0.0"
  }
}
