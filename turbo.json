{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputLogs": "new-only",
      "outputs": [".svelte-kit/**", "dist/**", "build/**", ".next/**", "target/**"]
    },
    "dev": {
      "cache": false,
      "outputLogs": "new-only",
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputLogs": "new-only"
    },
    "check": {
      "dependsOn": ["^build"],
      "outputLogs": "new-only"
    }
  }
}
