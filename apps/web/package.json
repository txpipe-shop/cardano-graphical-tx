    "@codemirror/view": "^6.39.9",
    "@heroui/accordion": "2.2.26",
    "@heroui/react": "2.8.7",
    "@heroui/tabs": "2.2.26",
    "@heroui/theme": "2.4.25",
    "@laceanatomy/cardano-provider-dbsync": "workspace:*",
    "@laceanatomy/napi-pallas": "workspace:*",
    "@laceanatomy/types": "workspace:*",
    "@next/third-parties": "^15.2.3",
    "@prisma/client": "^5.19.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "@uiw/react-codemirror": "^4.23.6",
    "autoprefixer": "^10.4.20",
    "axios": "^1.5.0",
    "bech32": "^2.0.0",
    "canvas": "^3.2.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.18.0",
    "http-status-codes": "^2.3.0",
    "json-bigint": "^1.0.0",
    "jsonpointer": "^5.0.1",
    "konva": "^9.3.13",
    "next": "^14.2.22",
    "next-auth": "^4.24.7",
    "next-themes": "^0.2.1",
    "pg": "^8.11.3",
    "react": "latest",
    "react-dom": "latest",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.8.0",
    "react-konva": "^18.2.10",
