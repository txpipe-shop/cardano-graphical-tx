{
  "name": "cardano-graphical-tx",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:pallas": "cd napi-pallas && yarn install --immutable && yarn build",
    "build": "next build",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "build:docker": "docker build --build-arg NEXT_PUBLIC_CBOR_ENDPOINT='https://laceanatomy.com/tx?_data=routes%2Ftx' -t graphical-tx .",
    "run:docker": "docker run -p 3000:3000 graphical-tx",
    "push:docker": "npm run build:docker && docker tag graphical-tx graphicaltx/cardano-graphical-tx:latest && docker push graphicaltx/cardano-graphical-tx:latest",
    "lint": "next lint",
    "clear": "rm -rf .next && rm -rf node_modules && rm -rf napi-pallas/node_modules",
    "start": "next start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
    "@codemirror/lang-json": "^6.0.1",
    "@nextui-org/react": "^2.4.6",
    "@prisma/client": "^5.19.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "@uiw/react-codemirror": "^4.23.6",
    "autoprefixer": "^10.4.20",
    "axios": "^1.5.0",
    "bech32": "^2.0.0",
    "canvas": "^2.11.2",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.18.0",
    "http-status-codes": "^2.3.0",
    "json-bigint": "^1.0.0",
    "jsonpointer": "^5.0.1",
    "konva": "^9.3.13",
    "napi-pallas": "./napi-pallas",
    "next": "latest",
    "next-auth": "^4.24.7",
    "next-themes": "^0.2.1",
    "react": "latest",
    "react-dom": "latest",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.8.0",
    "react-konva": "^18.2.10",
